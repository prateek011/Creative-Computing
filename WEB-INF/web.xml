<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Signupaction</servlet-name>
        <servlet-class>com.Signupaction</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Loginaction</servlet-name>
        <servlet-class>com.Loginaction</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Articlesubaction</servlet-name>
        <servlet-class>com.Articlesubaction</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Blogsubaction</servlet-name>
        <servlet-class>com.Blogsubaction</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Quersubaction</servlet-name>
        <servlet-class>com.Quersubaction</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ArticleShow</servlet-name>
        <servlet-class>com.ArticleShow</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BlogShow</servlet-name>
        <servlet-class>com.BlogShow</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>QueryShow</servlet-name>
        <servlet-class>com.QueryShow</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>QuesAnswer</servlet-name>
        <servlet-class>com.QuesAnswer</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogoutAction</servlet-name>
        <servlet-class>com.LogoutAction</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Signupaction</servlet-name>
        <url-pattern>/Signupaction</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Loginaction</servlet-name>
        <url-pattern>/Loginaction</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Articlesubaction</servlet-name>
        <url-pattern>/Articlesubaction</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Blogsubaction</servlet-name>
        <url-pattern>/Blogsubaction</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Quersubaction</servlet-name>
        <url-pattern>/Quersubaction</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ArticleShow</servlet-name>
        <url-pattern>/ArticleShow</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BlogShow</servlet-name>
        <url-pattern>/BlogShow</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>QueryShow</servlet-name>
        <url-pattern>/QueryShow</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>QuesAnswer</servlet-name>
        <url-pattern>/QuesAnswer</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogoutAction</servlet-name>
        <url-pattern>/LogoutAction</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>
</web-app>
